<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_fab_example"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="edu.monash.fab2081.MainActivity"
    tools:showIn="@layout/activity_main">

    <ListView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/listView"
        app:layout_constraintLeft_toLeftOf="@+id/content_fab_example"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/content_fab_example"
        android:layout_marginTop="64dp"
        app:layout_constraintRight_toRightOf="@+id/content_fab_example"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/content_fab_example"
        android:layout_marginBottom="16dp" />

</android.support.constraint.ConstraintLayout>
